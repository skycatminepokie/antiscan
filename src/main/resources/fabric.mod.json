{
	"schemaVersion": 1,
	"id": "ip-checker",
	"version": "${version}",
	"name": "IP Checker",
	"description": "Check IPs against AbuseIPDB before admitting people to your server.",
	"authors": [
		"skycatminepokie"
	],
	"contact": {
		"sources": "https://github.com/skycatminepokie/ip-checker"
	},
	"license": "MIT",
	"icon": "assets/ip-checker/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.skycatdev.ipchecker.IPChecker"
		],
		"client": [
			"com.skycatdev.ipchecker.IPCheckerClient"
		],
		"fabric-datagen": [
			"com.skycatdev.ipchecker.IPCheckerDataGenerator"
		]
	},
	"mixins": [
		"ip-checker.mixins.json",
		{
			"config": "ip-checker.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.12",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*"
	}
}