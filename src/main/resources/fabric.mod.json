{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Check IPs against AbuseIPDB before admitting people to your server.",
  "authors": [
    "skycatminepokie"
  ],
  "contact": {
    "sources": "https://github.com/skycatminepokie/antiscan"
  },
  "license": "MIT",
  "icon": "assets/antiscan/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      "com.skycatdev.antiscan.Antiscan"
    ],
    "fabric-datagen": [
      "com.skycatdev.antiscan.client.datagen.AntiscanDataGenerator"
    ]
  },
  "mixins": [
    "antiscan.mixins.json",
    {
      "config": "antiscan.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader}",
    "minecraft": "${minecraft}",
    "java": ">=${java}"
  }
}